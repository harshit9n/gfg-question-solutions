vector<int> countOddEven(int arr[], int sizeof_array)
    {
        // your code here
        vector<int> ans;
        int odd=0;
        int even =0;
        for(int i=0;i<sizeof_array;i++)
        {
            if(arr[i]%2 ==0)
            {
                even++;
            }
            else{
                odd++;
            }
        }
        ans.push_back(odd);
        ans.push_back(even);
        return ans;
    }
